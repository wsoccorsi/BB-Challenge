<%= simple_form_for @dog, :html=> { class: 'dogs-form' } do |f| %>
  <%= f.input :name %>
  <%= f.input :description, as: :text %>
  <%= f.input :images, as: :file, :input_html => { :multiple => true } %>

  <% @dog.images.each do |image| %>
    <%= image_tag image %>
  <% end %>

  <%= f.button :submit %>
<% end %>
